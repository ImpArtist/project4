<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.mapper.DbManageMapper">

    <select id="getTableData" resultType="java.util.LinkedHashMap">
        SELECT * FROM ${tableName}
            <if test="attribute != null and attribute != ''">
                WHERE ${attribute} LIKE CONCAT('%', #{value}, '%')
            </if>
    </select>

    <insert id="updateStructTable">
        INSERT INTO ${tableName} VALUES
        <foreach collection="data" item="item" separator="union all">
            (
                #{item.Field},#{item.Type},#{item.Null},#{item.Key},#{item.Default},#{item.Extra}
            )
        </foreach>
    </insert>

</mapper>